{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Deno",
        "type": "pwa-node",
        "request": "launch",
        "cwd": "${workspaceFolder}",
        "runtimeExecutable": "deno",
        "runtimeArgs": ["run", "--inspect-brk", "--allow-net=localhost", "--allow-read=content,out", "--allow-write=out", "main.ts"],
        "attachSimplePort": 9229
      }
    ]
  }